<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.rasberryrabbit" name="rasberryrabbit's XBMC Addons" version="0.1" provider-name="rasberryrabbit">
	<extension point="xbmc.addon.repository" name="rasberryrabbit's XBMC Addon Repository">
		<info compressed="false">https://raw.githubusercontent.com/rasberryrabbit/rasberryrabbit-xbmc-repo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/rasberryrabbit/rasberryrabbit-xbmc-repo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/rasberryrabbit/rasberryrabbit-xbmc-repo/master/repo/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install rasberryrabbit's Addons</summary>
		<forum/>
		<description>Download and install addons by rasberryrabbit</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="service.subtitles.22min"
       name="22min.com"
       version="0.8.2"
       provider-name="rasberryrabbit">
    <requires>
        <import addon="xbmc.python" version="2.14.0"/>
    </requires>
    <extension point="xbmc.subtitle.module"
               library="service.py"/>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Get subtitles from 22min.com</summary>
        <description lang="en">22min.com is search engine for Korean subtitles from DCInside and Bunyuc forum</description>
        <disclaimer lang="en"/>
        <platform>all</platform>
        <language/>
        <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
        <forum>http://22min.com</forum>
        <website/>
        <email/>
        <source>https://github.com/rasberryrabbit/service.subtitles.22min</source>
    </extension>
</addon>

