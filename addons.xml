<addon id="plugin.video.youtube" name="YouTube" version="5.1.17" provider-name="rasberryrabbit">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
